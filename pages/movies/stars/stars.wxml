<!--pages/star/star.wxml-->
<template name="starsTemplate">
  <view class='stars-container'>
    <view class='starts'>
    <block wx:for="{{stars}}" wx:for-item="i"> 
     <image wx:if="{{i}}" class='starts-img' src='/images/icon/star.png'></image>
     <image wx:else class='starts-img' src='/images/icon/none-star.png'></image>
    </block>
    </view>
    <text class='stars-score'>{{score}}</text>
  </view>
</template>